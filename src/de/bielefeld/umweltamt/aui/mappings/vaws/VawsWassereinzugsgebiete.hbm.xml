<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.vaws.VawsWassereinzugsgebiete" table="vaws_wassereinzugsgebiete" lazy="false" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="ezgbname" type="java.lang.String">
            <meta attribute="use-in-tostring" inherit="false">true</meta>
            <column name="ezgbname" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
        <set name="basisStandorts" table="basis_standort" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="wassereinzgebid" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.basis.BasisStandort" />
        </set>
    </class>
</hibernate-mapping>
