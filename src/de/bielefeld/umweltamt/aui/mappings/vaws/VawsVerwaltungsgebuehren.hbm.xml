<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.vaws" schema="auik">

    <class name="VawsVerwaltungsgebuehren" table="vaws_verwaltungsgebuehren">
        <id name="id" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
            	<param name="sequence">vaws_verwaltungsgebuehren_id_seq</param>
            </generator>
        </id>

		<many-to-one name="vawsFachdaten" column="behaelterid" class="VawsFachdaten" />
        <many-to-one name="vawsGebuehrenart" column="gebuehrenart" class="VawsGebuehrenarten" />

        <property name="datum" column="datum" type="java.util.Date" />
        <property name="betrag" column="betrag" type="java.lang.Float" />
        <property name="abschnitt" column="abschnitt" type="java.lang.String" />
        <property name="kassenzeichen" column="kassenzeichen" type="java.lang.String" />
        <property name="enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="deleted" column="_deleted" type="java.lang.Boolean" />

    </class>

</hibernate-mapping>