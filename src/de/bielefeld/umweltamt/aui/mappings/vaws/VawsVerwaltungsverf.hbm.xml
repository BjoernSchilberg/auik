<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.vaws.VawsVerwaltungsverf" table="vaws_verwaltungsverf" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native">
                <param name="sequence">auik.vaws_verwaltungsverf_id_seq</param>
            </generator>
        </id>
        <many-to-one name="vawsFachdaten" class="de.bielefeld.umweltamt.aui.mappings.vaws.VawsFachdaten" fetch="select">
            <column name="behaelterid" />
        </many-to-one>
        <property name="datum" type="java.util.Date">
            <column name="datum" length="8" />
        </property>
        <property name="massnahme" type="java.lang.String">
            <column name="massnahme" />
        </property>
        <property name="wiedervorlage" type="java.util.Date">
            <column name="wiedervorlage" length="8" />
        </property>
        <property name="wvverwverf" type="java.lang.Boolean">
            <column name="wvverwverf" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
