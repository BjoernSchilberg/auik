<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.vaws" schema="auik">

    <class name="VawsKontrollen" table="vaws_kontrollen">
        <id name="id" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
            	<param name="sequence">vaws_kontrollen_id_seq</param>
            </generator>
        </id>

        <property name="grundderpruefung" column="grundderpruefung" type="java.lang.String" />
        <property name="pruefturnus" column="pruefturnus" type="java.lang.Float" />
        <property name="pruefdatum" column="pruefdatum" type="java.util.Date" />
        <property name="pruefer" column="pruefer" type="java.lang.String" />
        <property name="pruefungabgeschlossen" column="pruefungabgeschlossen" type="java.lang.Boolean" />
        <property name="nachpruefung" column="nachpruefung" type="java.lang.Boolean" />
        <property name="nachpruefdatum" column="nachpruefdatum" type="java.util.Date" />
        <property name="nachpruefer" column="nachpruefer" type="java.lang.String" />
        <property name="naechstepruefung" column="naechstepruefung" type="java.util.Date" />
        <property name="pruefergebnis" column="pruefergebnis" type="java.lang.String" />

        <many-to-one name="vawsFachdaten" column="behaelterid" class="VawsFachdaten" />
    </class>

</hibernate-mapping>