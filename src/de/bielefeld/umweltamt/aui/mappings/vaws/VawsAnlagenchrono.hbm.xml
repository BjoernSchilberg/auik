<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.vaws">

    <class name="VawsAnlagenchrono" table="vaws_anlagenchrono">
        <id name="id" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
            	<param name="sequence">vaws_anlagenchrono_id_seq</param>
            </generator>
        </id>

        <property name="datum" column="datum" type="java.util.Date" />
        <property name="sachverhalt" column="sachverhalt" type="java.lang.String" />
        <property name="wv" column="wv" type="java.util.Date" />

        <many-to-one name="vawsFachdaten" column="behaelterid" class="VawsFachdaten" not-null="true" />
    </class>

</hibernate-mapping>