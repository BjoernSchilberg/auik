<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.atl" schema="auik">

    <class name="AtlAnalyseposition" table="atl_analyseposition">
        <id name="id" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">atl_analyseposition_id_seq</param>
        	</generator>
        </id>

        <property name="grkl" column="grkl" type="java.lang.String" />
        <property name="wert" column="wert" type="java.lang.Float" not-null="true" />
        <property name="analyseVon" column="analyse_von" type="java.lang.String" />
        <property name="bericht" column="bericht" type="java.lang.String" />
        <property name="normwert" column="normwert" type="java.lang.Double" />
        <property name="_enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="_deleted" column="_deleted" type="java.lang.Boolean" />

        <many-to-one name="atlEinheiten" column="einheiten_id" class="AtlEinheiten" />

        <many-to-one name="atlParameter" column="parameter_id" class="AtlParameter" not-null="true" />

        <many-to-one name="atlProbenahmen" column="probenahme_id" class="AtlProbenahmen" not-null="true" />
    </class>

</hibernate-mapping>