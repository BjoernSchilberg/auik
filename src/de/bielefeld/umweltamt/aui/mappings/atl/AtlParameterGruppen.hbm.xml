<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.atl">

    <class name="AtlParameterGruppen" table="atl_parametergruppen" lazy="false">
        <id name="id" type="java.lang.Integer">
            <column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">atl_parametergruppen_id_seq</param>
            </generator>
        </id>

        <property name="name" column="name" type="java.lang.String" />
        <property name="preisfueranalyse" column="preisfueranalyse" type="java.lang.Double"/>
    </class>

</hibernate-mapping>
