<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.atl">

    <class name="AtlKlaeranlagen" table="atl_klaeranlagen" lazy="false" mutable="false">
        <id name="kaId" column="id" type="java.lang.Integer">
            <generator class="assigned"/>
        </id>

        <property name="anlage" column="anlage" type="java.lang.String"  not-null="true" />
    </class>

</hibernate-mapping>