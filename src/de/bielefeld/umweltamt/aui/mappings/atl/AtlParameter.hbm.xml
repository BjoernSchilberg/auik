<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.atl" schema="auik">

    <class name="AtlParameter" table="atl_parameter" mutable="false" lazy="false">
        <id name="ordnungsbegriff" column="ordnungsbegriff" type="java.lang.String">
            <generator class="assigned"/>
        </id>

        <property name="analyseverfahren" column="analyseverfahren" type="java.lang.String" />
        <property name="bezeichnung" column="bezeichnung" type="java.lang.String" />
        <property name="wirdgemessenineinheit" column="wirdgemessenineinheit" type="java.lang.Integer" />
        <property name="grenzwert" column="grenzwert" type="java.lang.Double" />
        <property name="grenzwertname" column="grenzwertname" type="java.lang.String" />
        <property name="sielhautGw" column="sielhaut_gw" type="java.lang.Double" />
        <property name="klaerschlammGw" column="klaerschlamm_gw" type="java.lang.Double" />
        <property name="preisfueranalyse" column="preisfueranalyse" type="java.lang.Double" />
        <property name="einzelnBeauftragbar" column="einzelnBeauftragbar" type="java.lang.Boolean" />
        <property name="kennzeichnung" column="kennzeichnung" type="java.lang.String" />
        <property name="konservierung" column="konservierung" type="java.lang.String" />
        <property name="reihenfolge" column="reihenfolge" type="java.lang.Integer" />
        <property name="_enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="_deleted" column="_deleted" type="java.lang.Boolean" />

        <many-to-one name="atlParameterGruppe" column="parametergruppe_id" class="AtlParameterGruppen" />
    </class>

</hibernate-mapping>
