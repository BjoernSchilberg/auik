<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.atl">

    <class name="AtlProbeart" table="atl_probeart" mutable="false" lazy="false">
        <id name="artId" column="id" type="java.lang.Integer">
            <generator class="assigned"/>
        </id>
 
        <property name="art" column="art" type="java.lang.String"  not-null="true" />
    </class>
    
</hibernate-mapping>
