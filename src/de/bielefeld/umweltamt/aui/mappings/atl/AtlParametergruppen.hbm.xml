<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.atl.AtlParametergruppen" table="atl_parametergruppen" lazy="false" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
        <property name="preisfueranalyse" type="java.lang.Double">
            <column name="preisfueranalyse" precision="8" scale="0" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
        <set name="atlParameters" table="atl_parameter" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="parametergruppe_id" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.atl.AtlParameter" />
        </set>
    </class>
</hibernate-mapping>
