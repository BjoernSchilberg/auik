   <column name="_enabled" not-null="true"/>
  </property>
  <property generated="never" lazy="false" name="deleted" type="boolean">
   <column name="_deleted" not-null="true"/>
  </property>
  <set fetch="select" inverse="true" lazy="true"
   name="atlAnalysepositions" sort="unsorted" table="atl_analyseposition">
   <key>
    <column name="probenahme_id" not-null="true"/>
   </key>
   <one-to-many class="de.bielefeld.umweltamt.aui.mappings.atl.AtlAnalyseposition"/>
  </set>
 </class>
</hibernate-mapping>
