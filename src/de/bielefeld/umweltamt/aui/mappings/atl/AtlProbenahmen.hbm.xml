<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.atl" schema="auik">

    <class name="AtlProbenahmen" table="atl_probenahmen" lazy="false">
        <id name="id" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">atl_probenahmen_id_seq</param>
             </generator>
        </id>

        <property name="kennummer" column="kennummer" type="java.lang.String" />   
        <property name="art" column="art" type="java.lang.String" />
        <property name="datumDerEntnahme" column="datum_der_entnahme" type="java.sql.Timestamp" />
        <property name="zeitAnfang" column="zeit_anfang" type="java.sql.Time" />
        <property name="bisDatum" column="bis_datum" type="java.util.Date" />
        <property name="zeitDerEntnahmen" column="zeit_der_entnahmen" type="java.lang.String" />
        <property name="einwaage" column="einwaage" type="java.lang.Float" />
        <property name="vm3" column="v_m3" type="java.lang.String" />
        <property name="fahrer" column="fahrer" type="java.lang.String" />
        <property name="objektNr" column="objekt_nr" type="java.lang.String" />
        <property name="datumIcp" column="datum_icp" type="java.util.Date" />
        <property name="sonderparameter" column="sonderparameter" type="java.lang.String" />
        <property name="bemerkung" column="bemerkung" type="java.lang.String" />
        <property name="an360x11" column="an_360x11" type="java.util.Date" />
        <property name="ueberschreitung" column="ueberschreitung" type="java.lang.String" />
        <property name="anzahlbeteiligte" column="anzahlbeteiligte" type="java.lang.Integer" />
        <property name="uhrzeitbeginn" column="uhrzeitbeginn" type="java.lang.String" />
        <property name="uhrzeitende" column="uhrzeitende" type="java.lang.String" />
        <property name="fahrtzeit" column="fahrtzeit" type="java.lang.String" />
        <property name="bescheid" column="bescheid" type="java.util.Date" />
        <property name="kosten" column="kosten" type="java.lang.Double" />
		<property name="sachbearbeiter" column="sachbearbeiter" type="java.lang.String" />
        <property name="massnahmen" column="massnahmen" type="java.lang.String" />
        <property name="bezeichnung" column="bezeichnung" type="java.lang.String" />

        <many-to-one name="atlProbepkt" column="objektid" class="AtlProbepkt" />

        <many-to-one name="atlStatus" column="status" class="AtlStatus" />

        <set name="atlAnalysepositionen" lazy="true" inverse="true" cascade="all-delete-orphan">
        	<key column="probenahme_id"/>
        	<one-to-many class="AtlAnalyseposition"/>
    	</set>
    </class>

</hibernate-mapping>