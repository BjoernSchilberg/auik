  <property generated="never" lazy="false" name="ueberschreitung" type="java.lang.String">
   <column name="ueberschreitung"/>
  </property>
  <property generated="never" lazy="false" name="anzahlbeteiligte" type="java.lang.Integer">
   <column name="anzahlbeteiligte"/>
  </property>
  <property generated="never" lazy="false" name="uhrzeitbeginn" type="java.lang.String">
   <column name="uhrzeitbeginn"/>
  </property>
  <property generated="never" lazy="false" name="uhrzeitende" type="java.lang.String">
