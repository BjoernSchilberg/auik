<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.atl.AtlStatus" table="atl_status" lazy="false" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="bezeichnung" type="java.lang.String">
            <meta attribute="use-in-tostring" inherit="false">true</meta>
            <column name="bezeichnung" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
        <set name="atlProbenahmens" table="atl_probenahmen" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="status" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.atl.AtlProbenahmen" />
        </set>
    </class>
</hibernate-mapping>
