<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.atl.AtlSielhaut" table="atl_sielhaut" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native">
                <param name="sequence">auik.atl_sielhaut_id_seq</param>
            </generator>
        </id>
        <many-to-one name="basisObjekt" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" fetch="select">
            <column name="objektid" not-null="true" />
        </many-to-one>
        <many-to-one name="atlProbepkt" class="de.bielefeld.umweltamt.aui.mappings.atl.AtlProbepkt" update="false" insert="false" fetch="select">
            <column name="objektid" not-null="true" />
        </many-to-one>
        <property name="bezeichnung" type="java.lang.String">
            <column name="bezeichnung" />
        </property>
        <property name="haltungsnr" type="java.lang.String">
            <column name="haltungsnr" />
        </property>
        <property name="alarmplannr" type="java.lang.String">
            <column name="alarmplannr" />
        </property>
        <property name="entgeb" type="java.lang.String">
            <column name="entgeb" />
        </property>
        <property name="rechtswert" type="java.lang.Double">
            <column name="rechtswert" precision="8" scale="0" />
        </property>
        <property name="hochwert" type="java.lang.Double">
            <column name="hochwert" precision="8" scale="0" />
        </property>
        <property name="lage" type="java.lang.String">
            <column name="lage" />
        </property>
        <property name="bemerkungen" type="java.lang.String">
            <column name="bemerkungen" />
        </property>
        <property name="twabfluss" type="java.lang.String">
            <column name="twabfluss" />
        </property>
        <property name="bsb" type="java.lang.String">
            <column name="bsb" />
        </property>
        <property name="ew" type="java.lang.String">
            <column name="ew" />
        </property>
        <property name="gebiet" type="java.lang.String">
            <column name="gebiet" />
        </property>
        <property name="PSielhaut" type="java.lang.Boolean">
            <column name="p_sielhaut" />
        </property>
        <property name="PAlarmplan" type="java.lang.Boolean">
            <column name="p_alarmplan" />
        </property>
        <property name="PNachprobe" type="java.lang.Boolean">
            <column name="p_nachprobe" />
        </property>
        <property name="schlammprobe" type="java.lang.Boolean">
            <column name="schlammprobe" />
        </property>
        <property name="PFirmenprobe" type="java.lang.Boolean">
            <column name="p_firmenprobe" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
        <set name="atlProbepkts" table="atl_probepkt" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="sielhaut_id" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.atl.AtlProbepkt" />
        </set>
    </class>
</hibernate-mapping>
