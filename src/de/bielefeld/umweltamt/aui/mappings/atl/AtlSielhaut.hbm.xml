<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.atl" schema="auik">

    <class name="AtlSielhaut" table="atl_sielhaut">
        <id name="id" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">atl_sielhaut_id_seq</param>
        	</generator>
        </id>
        <many-to-one name="basisObjekt" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" fetch="select">
            <column name="objektid" not-null="true" />
        </many-to-one>
        <many-to-one name="atlProbepkt" class="de.bielefeld.umweltamt.aui.mappings.atl.AtlProbepkt" update="false" insert="false" fetch="select">
            <column name="objektid" not-null="true" />
        </many-to-one>
        <property name="bezeichnung" column="bezeichnung" type="java.lang.String" />
        <property name="haltungsnr" column="haltungsnr" type="java.lang.String" />
        <property name="alarmplannr" column="alarmplannr" type="java.lang.String" />
        <property name="entgeb" column="entgeb" type="java.lang.String" />
        <property name="rechtswert" column="rechtswert" type="java.lang.Double" />
        <property name="hochwert" column="hochwert" type="java.lang.Double" />
        <property name="lage" column="lage" type="java.lang.String" />
        <property name="bemerkungen" column="bemerkungen" type="java.lang.String" />
        <property name="twabfluss" column="twabfluss" type="java.lang.String" />
        <property name="bsb" column="bsb" type="java.lang.String" />
        <property name="ew" column="ew" type="java.lang.String" />
        <property name="gebiet" column="gebiet" type="java.lang.String" />
        <property name="psielhaut" column="p_sielhaut" type="java.lang.Boolean" />
        <property name="palarmplan" column="p_alarmplan" type="java.lang.Boolean" />
        <property name="pnachprobe" column="p_nachprobe" type="java.lang.Boolean" />
        <property name="schlammprobe" column="schlammprobe" type="java.lang.Boolean" />
        <property name="pfirmenprobe" column="p_firmenprobe" type="java.lang.Boolean" />
        <property name="_enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="_deleted" column="_deleted" type="java.lang.Boolean" />

    </class>

</hibernate-mapping>