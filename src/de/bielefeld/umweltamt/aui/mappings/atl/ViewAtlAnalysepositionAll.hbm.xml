<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.atl.ViewAtlAnalysepositionAll" table="view_atl_analyseposition_all" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="grkl" type="java.lang.String">
            <column name="grkl" />
        </property>
        <property name="wert" type="java.lang.Float">
            <column name="wert" precision="4" scale="0" />
        </property>
        <property name="analyseVon" type="java.lang.String">
            <column name="analyse_von" />
        </property>
        <property name="bericht" type="java.lang.String">
            <column name="bericht" />
        </property>
        <property name="normwert" type="java.lang.Double">
            <column name="normwert" precision="8" scale="0" />
        </property>
        <property name="einheitenId" type="java.lang.Integer">
            <column name="einheiten_id" />
        </property>
        <property name="parameterId" type="java.lang.String">
            <column name="parameter_id" />
        </property>
        <property name="probenahmeId" type="java.lang.Integer">
            <column name="probenahme_id" />
        </property>
        <property name="enabled" type="java.lang.Boolean">
            <column name="_enabled" />
        </property>
        <property name="deleted" type="java.lang.Boolean">
            <column name="_deleted" />
        </property>
        <property name="datumDerEntnahme" type="java.util.Date">
            <column name="datum_der_entnahme" length="8" />
        </property>
        <property name="probepktId" type="java.lang.Integer">
            <column name="probepkt_id" />
        </property>
        <property name="kennummer" type="java.lang.String">
            <column name="kennummer" />
        </property>
    </class>
</hibernate-mapping>
