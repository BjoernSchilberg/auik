<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.indeinl" schema="auik">

    <class name="Anh50Fachdaten" table="anh_50_fachdaten">
        <id name="objektid" column="objektid" type="java.lang.Integer">
            <generator class="foreign">
            	<param name="property">basisObjekt</param>
            </generator>
        </id>

        <property name="telefon" column="telefon" type="java.lang.String" />
        <property name="erloschen" column="erloschen" type="java.lang.Boolean" />
        <property name="datumantrag" column="datumantrag" type="java.util.Date" />
        <property name="bemerkungen" column="bemerkungen" type="java.lang.String" />
        <property name="genehmigung" column="genehmigung" type="java.util.Date" />
        <property name="wiedervorlage" column="wiedervorlage" type="java.util.Date" />
        <property name="gefaehrdungsklasse" column="gefaehrdungsklasse" type="java.lang.String" />
        <property name="_enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="_deleted" column="_deleted" type="java.lang.Boolean" />

        <many-to-one name="anhEntsorger" column="entsorgerid" class="AnhEntsorger" />

        <one-to-one name="basisObjekt" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" constrained="true" cascade="all"/>
    </class>

</hibernate-mapping>
