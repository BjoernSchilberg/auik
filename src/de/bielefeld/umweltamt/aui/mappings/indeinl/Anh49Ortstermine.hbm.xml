<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.indeinl" schema="auik">

    <class name="Anh49Ortstermine" table="anh_49_ortstermine">
        <id name="ortsterminid" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">anh_49_ortstermine_id_seq</param>
        	</generator>
        </id>

        <property name="datum" column="datum" type="java.util.Date" />
        <property name="sachbearbeiterIn" column="sachbearbeiter_in" type="java.lang.String" />
        <property name="bemerkungen" column="bemerkungen" type="java.lang.String" />

        <many-to-one name="anh49Fachdaten" column="anh49id" class="Anh49Fachdaten" />

    </class>

</hibernate-mapping>
