<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.indeinl">

    <class name="Anh52Fachdaten" table="anh_52_fachdaten">
        <id name="id" column="objektid" type="java.lang.Integer">
            <generator class="foreign">
            	<param name="property">basisObjekt</param>
            </generator>
        </id>

		<property name="nrbetriebsstaette" column="nrbetriebsstaette" type="java.lang.Integer" />
		<property name="firmenname" column="firmenname" type="java.lang.String" />
		<property name="telefon" column="telefon" type="java.lang.String" />
		<property name="telefax" column="telefax" type="java.lang.String" />
		<property name="ansprechpartner" column="ansprechpartner" type="java.lang.String" />
		<property name="datumGen" column="datumgenehmigung" type="java.util.Date" />
        <property name="bemerkungen" column="bemerkungen" type="java.lang.String" />

        <one-to-one name="basisObjekt" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" />
    </class>

</hibernate-mapping>