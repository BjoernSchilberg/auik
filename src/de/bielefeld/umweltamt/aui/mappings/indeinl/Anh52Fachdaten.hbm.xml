<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh52Fachdaten" table="anh_52_fachdaten" schema="auik">
        <id name="objektid" type="java.lang.Integer">
            <column name="objektid" />
            <generator class="foreign">
                <param name="property">basisObjekt</param>
            </generator>
        </id>
        <one-to-one name="basisObjekt" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" constrained="true" cascade="all"></one-to-one>
        <property name="nrbetriebsstaette" type="java.lang.Integer">
            <column name="nrbetriebsstaette" />
        </property>
        <property name="firmenname" type="java.lang.String">
            <column name="firmenname" />
        </property>
        <property name="telefon" type="java.lang.String">
            <column name="telefon" />
        </property>
        <property name="telefax" type="java.lang.String">
            <column name="telefax" />
        </property>
        <property name="ansprechpartner" type="java.lang.String">
            <column name="ansprechpartner" />
        </property>
        <property name="datumgenehmigung" type="java.util.Date">
            <column name="datumgenehmigung" length="8" />
        </property>
        <property name="bemerkungen" type="java.lang.String">
            <column name="bemerkungen" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
