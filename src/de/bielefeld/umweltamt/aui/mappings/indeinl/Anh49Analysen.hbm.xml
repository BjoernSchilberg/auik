<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.indeinl" schema="auik">

    <class name="Anh49Analysen" table="anh_49_analysen">
        <id name="analysenid" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">anh_49_abscheiderdetails_id_seq</param>
        	</generator>
        </id>

        <property name="datum" column="datum" type="java.util.Date" />
        <property name="institut" column="institut" type="java.lang.String" />
        <property name="csbWert" column="csb_wert" type="java.lang.String" />
        <property name="csbGrenzwert" column="csb_grenzwert" type="java.lang.String" />
        <property name="aoxWert" column="aox_wert" type="java.lang.String" />
        <property name="aoxGrenzwert" column="aox_grenzwert" type="java.lang.String" />
        <property name="kwWert" column="kw_wert" type="java.lang.String" />
        <property name="kwGrenzwert" column="kw_grenzwert" type="java.lang.String" />
        <property name="zinkWert" column="zink_wert" type="java.lang.String" />
        <property name="zinkGrenzwert" column="zink_grenzwert" type="java.lang.String" />
        <property name="bsb5Wert" column="bsb5_wert" type="java.lang.String" />
        <property name="bsb5CsbRelation" column="bsb5_csb_relation" type="java.lang.String" />
        <property name="bemerkungen" column="bemerkungen" type="java.lang.String" />
        <property name="bikGrenzwert" column="bik_grenzwert" type="java.lang.String" />
        <property name="bikWert" column="bik_wert" type="java.lang.String" />
        <property name="phWert" column="ph_wert" type="java.lang.String" />
        <property name="phGrenzwert" column="ph_grenzwert" type="java.lang.String" />
        <property name="enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="deleted" column="_deleted" type="java.lang.Boolean" />
 		
        <many-to-one name="anh49Fachdaten" column="anh49id" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh49Fachdaten" />

    </class>

</hibernate-mapping>
