<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.indeinl" schema="auik">

    <class name="Anh49Verwaltungsverfahren" table="anh_49_verwaltungsverf">
        <id name="VerwaltungsverfahrenID" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">anh_49_verwaltungsverf_id_seq</param>
        	</generator>
        </id>

        <property name="datum" column="datum" type="java.util.Date" />
        <property name="massnahme" column="massnahme" type="java.lang.String" />
        <property name="sachbearbeiterIn" column="sachbearbeiter_in" type="java.lang.String" />
        <property name="wiedervorlage" column="wiedervorlage" type="java.util.Date" />
        <property name="abgeschlossen" column="abgeschlossen" type="java.lang.Boolean" />
        <property name="enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="deleted" column="_deleted" type="java.lang.Boolean" />

        <many-to-one name="anh49Fachdaten" column="anh49id" class="Anh49Fachdaten" />

    </class>

</hibernate-mapping>
