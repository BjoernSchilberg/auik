            </generator>
        </id>

        <property name="klaeranlage" column="klaeranlage" type="java.lang.String" />
        <property name="name" column="name" type="java.lang.String" />
        <property name="bemerkungen" column="bemerkungen" type="java.lang.String" />
        <property name="planquadrat" column="planquadrat" type="java.lang.String" />
