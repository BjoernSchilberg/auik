        <property name="werkstatt" column="werkstatt" type="java.lang.Boolean" />
        <property name="bodeneinlaeufe" column="bodeneinlaeufe" type="java.lang.Boolean" />
        <property name="waschanlagen" column="waschanlagen" type="java.lang.Boolean" />
        <property name="sonstiges" column="sonstiges" type="java.lang.String" />
