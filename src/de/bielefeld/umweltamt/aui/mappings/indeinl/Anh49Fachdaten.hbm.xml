        <property name="dekraTuevDatum" column="dekra_tuev_datum" type="java.util.Date" />
        
        <one-to-one name="basisObjekt" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" constrained="true" cascade="all"/>
        
        <set name="anh49Analysen" lazy="true" inverse="true" cascade="all-delete-orphan">
        	<key column="anh49id"/>
        	<one-to-many class="Anh49Analysen"/>
    	</set>
    </class>

</hibernate-mapping>