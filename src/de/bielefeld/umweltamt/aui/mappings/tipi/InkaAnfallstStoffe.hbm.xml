<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.tipi.InkaAnfallstStoffe" table="inka_anfallst_stoffe" schema="tipi">
        <composite-id name="id" class="de.bielefeld.umweltamt.aui.mappings.tipi.InkaAnfallstStoffeId">
            <key-property name="anfallstelleNr" type="java.lang.Integer">
                <column name="anfallstelle_nr" />
            </key-property>
            <key-property name="stoffNr" type="java.lang.Integer">
                <column name="stoff_nr" />
            </key-property>
        </composite-id>
        <property name="anfallstelleVer" type="java.lang.Integer">
            <column name="anfallstelle_ver" />
        </property>
        <property name="stoffVer" type="java.lang.Integer">
            <column name="stoff_ver" />
        </property>
        <property name="anfallstStoffeVer" type="java.lang.Integer">
            <column name="anfallst_stoffe_ver" />
        </property>
        <property name="gueltigVon" type="java.util.Calendar">
            <column name="gueltig_von" length="4" />
        </property>
        <property name="gueltigBis" type="java.util.Calendar">
            <column name="gueltig_bis" length="4" />
        </property>
        <property name="aenderungsDatum" type="java.util.Calendar">
            <column name="aenderungs_datum" length="8" />
        </property>
        <property name="erfassungsDatum" type="java.util.Calendar">
            <column name="erfassungs_datum" length="8" />
        </property>
        <property name="historienNr" type="java.lang.Integer">
            <column name="historien_nr" />
        </property>
        <property name="istAktuellJn" type="java.lang.Boolean">
            <column name="ist_aktuell_jn" />
        </property>
    </class>
</hibernate-mapping>
