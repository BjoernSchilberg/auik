<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.tipi.AuikWzCode" table="auik_wz_code" lazy="false" schema="tipi">
        <id name="bezeichnung" type="string">
            <column name="bezeichnung" />
            <generator class="assigned" />
        </id>
        <many-to-one name="deaWzCode" class="de.bielefeld.umweltamt.aui.mappings.tipi.DeaWzCode" fetch="select">
            <column name="wzcd_id" length="7" />
            <column name="wzcd_version" />
        </many-to-one>
        <property name="inKurzAuswahl" type="boolean">
            <column name="in_kurz_auswahl" not-null="true" />
        </property>
        <property name="ebene" type="int">
            <column name="ebene" not-null="true" />
        </property>
        <set name="basisBetreibers" table="basis_betreiber" schema="auik" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="auik_wz_code" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.basis.BasisBetreiber" />
        </set>
    </class>
</hibernate-mapping>
