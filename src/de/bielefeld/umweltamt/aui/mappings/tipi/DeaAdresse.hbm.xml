<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.tipi.DeaAdresse" table="dea_adresse" schema="tipi">
        <id name="adresseNr" type="java.lang.Integer">
            <column name="adresse_nr" />
            <generator class="assigned" />
        </id>
        <property name="adresseVer" type="java.lang.Integer">
            <column name="adresse_ver" />
        </property>
        <property name="gueltigVon" type="java.util.Calendar">
            <column name="gueltig_von" length="4" />
        </property>
        <property name="gueltigBis" type="java.util.Calendar">
            <column name="gueltig_bis" length="4" />
        </property>
        <property name="aenderungsDatum" type="java.util.Calendar">
            <column name="aenderungs_datum" length="8" />
        </property>
        <property name="erfassungsDatum" type="java.util.Calendar">
            <column name="erfassungs_datum" length="8" />
        </property>
        <property name="historienNr" type="java.lang.Integer">
            <column name="historien_nr" />
        </property>
        <property name="istAktuellJn" type="java.lang.Boolean">
            <column name="ist_aktuell_jn" />
        </property>
        <property name="name1" type="string">
            <column name="name1" length="40" />
        </property>
        <property name="name2" type="string">
            <column name="name2" length="40" />
        </property>
        <property name="strasse" type="string">
            <column name="strasse" length="40" />
        </property>
        <property name="hausnr" type="string">
            <column name="hausnr" length="15" />
        </property>
        <property name="plz" type="string">
            <column name="plz" length="10" />
        </property>
        <property name="ort" type="string">
            <column name="ort" length="40" />
        </property>
        <property name="staatskennung" type="string">
            <column name="staatskennung" length="3" />
        </property>
        <property name="telefon" type="string">
            <column name="telefon" length="30" />
        </property>
        <property name="telefonMobil" type="string">
            <column name="telefon_mobil" length="30" />
        </property>
        <property name="fax" type="string">
            <column name="fax" length="30" />
        </property>
        <property name="email" type="string">
            <column name="email" />
        </property>
    </class>
</hibernate-mapping>
