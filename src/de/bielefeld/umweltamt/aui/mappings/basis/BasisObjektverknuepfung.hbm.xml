<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.basis" schema="auik">

    <class name="BasisObjektverknuepfung" schema="auik" table="basis_objektverknuepfung">
        <id name="id" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">basis_objektverknuepfung_id_seq</param>
        	</generator>
        </id>
        <many-to-one name="basisObjektByIstVerknuepftMit" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" fetch="select">
            <column name="ist_verknuepft_mit" />
        </many-to-one>
        <many-to-one name="basisObjektByObjekt" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" fetch="select">
            <column name="objekt" />
        </many-to-one>
    </class>
</hibernate-mapping>
