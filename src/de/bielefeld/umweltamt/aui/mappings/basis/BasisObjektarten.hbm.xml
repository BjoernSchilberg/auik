<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjektarten" table="basis_objektarten" lazy="false" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="objektart" type="string">
            <column name="objektart" />
        </property>
        <property name="abteilung" type="string">
            <column name="abteilung" />
        </property>
        <property name="kategorie" type="string">
            <column name="kategorie" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
        <set name="basisObjekts" table="basis_objekt" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="objektartid" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" />
        </set>
    </class>
</hibernate-mapping>
