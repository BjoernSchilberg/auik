<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.basis" schema="auik">

    <class name="BasisStrassen" table="basis_strassen" mutable="false">
        <id name="strasseid" column="id" type="java.lang.Integer">
            <generator class="assigned"/>
        </id>

        <property name="strasse" column="strasse" type="java.lang.String" />
        <property name="plz" column="plz" type="java.lang.String" />
    </class>

</hibernate-mapping>