<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.basis" schema="auik">

    <class name="BasisStandort" table="basis_standort" lazy="false">
        <id name="standortid" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">basis_standort_id_seq</param>
        	</generator>
        </id>

        <property name="strasse" column="strasse" type="java.lang.String" />
        <property name="hausnr" column="hausnr" type="java.lang.Integer" />
        <property name="hausnrzus" column="hausnrzus" type="java.lang.String" />
        <property name="plz" column="plz" type="java.lang.String" />
        <property name="rechtswert" column="rechtswert" type="java.lang.Float" />
        <property name="hochwert" column="hochwert" type="java.lang.Float" />
        <property name="flur" column="flur" type="java.lang.String" />
        <property name="flurstueck" column="flurstueck" type="java.lang.String" />
        <property name="entgebid" column="entgebid" type="java.lang.String" />
        <property name="vornameeigent" column="vornameeigent" type="java.lang.String" />
        <property name="nameeigent" column="nameeigent" type="java.lang.String" />
        <property name="strasseeigent" column="strasseeigent" type="java.lang.String" />
        <property name="hausnreigent" column="hausnreigent" type="java.lang.Integer" />
        <property name="hausnrzuseigent" column="hausnrzuseigent" type="java.lang.String" />
        <property name="revidatum" column="revidatum" type="java.util.Date" />
        <property name="revihandz" column="revihandz" type="java.lang.String" />
        <property name="wassermenge" column="wassermenge" type="java.lang.Integer" />
        <property name="sachbe33rav" column="sachbe33rav" type="java.lang.String" />
        <property name="sachbe33hee" column="sachbe33hee" type="java.lang.String" />
        <property name="_enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="_deleted" column="_deleted" type="java.lang.Boolean" />

        <many-to-one name="basisGemarkung" column="gemarkungid" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisGemarkung" />

        <many-to-one name="vawsStandortgghwsg" column="standortgegebid" class="de.bielefeld.umweltamt.aui.mappings.vaws.VawsStandortgghwsg" />

        <many-to-one name="vawsWassereinzugsgebiete" column="wassereinzgebid" class="de.bielefeld.umweltamt.aui.mappings.vaws.VawsWassereinzugsgebiete" />

    </class>

</hibernate-mapping>