<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.basis.BasisStandort" table="basis_standort" lazy="false" schema="auik">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native">
                <param name="sequence">auik.basis_standort_id_seq</param>
            </generator>
        </id>
        <many-to-one name="basisGemarkung" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisGemarkung" fetch="select">
            <column name="gemarkungid" />
        </many-to-one>
        <many-to-one name="vawsWassereinzugsgebiete" class="de.bielefeld.umweltamt.aui.mappings.vaws.VawsWassereinzugsgebiete" fetch="select">
            <column name="wassereinzgebid" />
        </many-to-one>
        <many-to-one name="vawsStandortgghwsg" class="de.bielefeld.umweltamt.aui.mappings.vaws.VawsStandortgghwsg" fetch="select">
            <column name="standortgegebid" />
        </many-to-one>
        <property name="strasse" type="java.lang.String">
            <column name="strasse" />
        </property>
        <property name="hausnr" type="java.lang.Integer">
            <column name="hausnr" />
        </property>
        <property name="hausnrzus" type="java.lang.String">
            <column name="hausnrzus" />
        </property>
        <property name="plz" type="java.lang.String">
            <column name="plz" />
        </property>
        <property name="rechtswert" type="java.lang.Float">
            <column name="rechtswert" precision="4" scale="0" />
        </property>
        <property name="hochwert" type="java.lang.Float">
            <column name="hochwert" precision="4" scale="0" />
        </property>
        <property name="flur" type="java.lang.String">
            <column name="flur" />
        </property>
        <property name="flurstueck" type="java.lang.String">
            <column name="flurstueck" />
        </property>
        <property name="entgebid" type="java.lang.String">
            <column name="entgebid" />
        </property>
        <property name="vornameeigent" type="java.lang.String">
            <column name="vornameeigent" />
        </property>
        <property name="nameeigent" type="java.lang.String">
            <column name="nameeigent" />
        </property>
        <property name="strasseeigent" type="java.lang.String">
            <column name="strasseeigent" />
        </property>
        <property name="hausnreigent" type="java.lang.Integer">
            <column name="hausnreigent" />
        </property>
        <property name="hausnrzuseigent" type="java.lang.String">
            <column name="hausnrzuseigent" />
        </property>
        <property name="revidatum" type="java.util.Date">
            <column name="revidatum" length="8" />
        </property>
        <property name="revihandz" type="java.lang.String">
            <column name="revihandz" />
        </property>
        <property name="wassermenge" type="java.lang.Integer">
            <column name="wassermenge" />
        </property>
        <property name="sachbe33rav" type="java.lang.String">
            <column name="sachbe33rav" />
        </property>
        <property name="sachbe33hee" type="java.lang.String">
            <column name="sachbe33hee" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
        <set name="basisObjekts" table="basis_objekt" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="standortid" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" />
        </set>
    </class>
</hibernate-mapping>
