<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.basis" schema="auik">

    <class name="BasisObjekt" table="basis_objekt" lazy="false">
        <id name="objektid" type="java.lang.Integer">
        	<column name="objektid" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">basis_objekt_objektid_seq</param>
        	</generator>
        </id>

        <property name="uschistdid" column="uschistdid" type="java.lang.Integer" />
		<property name="beschreibung" column="beschreibung" type="java.lang.String" />
		<property name="sachbearbeiter" column="sachbearbeiter" type="java.lang.String" />
        <property name="wiedervorlage" column="wiedervorlage" type="java.util.Date" />
        <property name="erfassungsdatum" column="erfassungsdatum" type="java.util.Date" />
        <property name="gueltigVon" column="gueltig_von" type="java.util.Date" />
        <property name="aenderungsdatum" column="aenderungsdatum" type="java.util.Date" />
        <property name="gueltigBis" column="gueltig_bis" type="java.util.Date" />
        <property name="inaktiv" column="inaktiv" type="java.lang.Boolean" />

        <many-to-one name="basisBetreiber" column="betreiberid" class="BasisBetreiber" />

        <many-to-one name="basisObjektarten" column="objektartid" class="BasisObjektarten" />

        <many-to-one name="basisStandort" column="standortid" class="BasisStandort" />
        
        <one-to-one name="atlProbepkt" class="de.bielefeld.umweltamt.aui.mappings.atl.AtlProbepkt" />

    </class>

</hibernate-mapping>