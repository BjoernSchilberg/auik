<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjekt" table="basis_objekt" lazy="false" schema="auik">
        <id name="objektid" type="java.lang.Integer">
            <column name="objektid" />
            <generator class="native">
                <param name="sequence">auik.basis_objekt_objektid_seq</param>
            </generator>
        </id>
        <many-to-one name="basisBetreiber" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisBetreiber" fetch="select">
            <column name="betreiberid" />
        </many-to-one>
        <many-to-one name="basisStandort" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisStandort" fetch="select">
            <column name="standortid" />
        </many-to-one>
        <many-to-one name="basisSachbearbeiter" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisSachbearbeiter" fetch="select">
            <column name="sachbearbeiter" />
        </many-to-one>
        <many-to-one name="basisObjektarten" class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjektarten" fetch="select">
            <column name="objektartid" />
        </many-to-one>
        <property name="uschistdid" type="java.lang.Integer">
            <column name="uschistdid" />
        </property>
        <property name="beschreibung" type="java.lang.String">
            <column name="beschreibung" />
        </property>
        <property name="wiedervorlage" type="java.util.Date">
            <column name="wiedervorlage" length="8" />
        </property>
        <property name="erfassungsdatum" type="java.util.Date">
            <column name="erfassungsdatum" length="8" />
        </property>
        <property name="gueltigVon" type="java.util.Date">
            <column name="gueltig_von" length="8" />
        </property>
        <property name="aenderungsdatum" type="java.util.Date">
            <column name="aenderungsdatum" length="8" />
        </property>
        <property name="gueltigBis" type="java.util.Date">
            <column name="gueltig_bis" length="8" />
        </property>
        <property name="inaktiv" type="boolean">
            <column name="inaktiv" not-null="true" />
        </property>
        <property name="prioritaet" type="java.lang.String">
            <column name="prioritaet" length="0" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
        <one-to-one name="indeinlGenehmigung" class="de.bielefeld.umweltamt.aui.mappings.indeinl.IndeinlGenehmigung"></one-to-one>
        <one-to-one name="anh53Fachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh53Fachdaten"></one-to-one>
        <one-to-one name="anhSuevFachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.AnhSuevFachdaten"></one-to-one>
        <one-to-one name="anhBwkFachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.AnhBwkFachdaten"></one-to-one>
        <one-to-one name="atlProbepkt" class="de.bielefeld.umweltamt.aui.mappings.atl.AtlProbepkt"></one-to-one>
        <set name="basisObjektverknuepfungsForObjekt" table="basis_objektverknuepfung" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="objekt" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjektverknuepfung" />
        </set>
        <set name="vawsFachdatens" table="vaws_fachdaten" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="objektid" not-null="true" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.vaws.VawsFachdaten" />
        </set>
        <one-to-one name="anh55Fachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh55Fachdaten"></one-to-one>
        <one-to-one name="anh52Fachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh52Fachdaten"></one-to-one>
        <one-to-one name="anh56Fachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh56Fachdaten"></one-to-one>
        <set name="basisObjektverknuepfungsForIstVerknuepftMit" table="basis_objektverknuepfung" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="ist_verknuepft_mit" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjektverknuepfung" />
        </set>
        <one-to-one name="anh40Fachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh40Fachdaten"></one-to-one>
        <set name="basisObjektchronos" table="basis_objektchrono" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="objektid" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.basis.BasisObjektchrono" />
        </set>
        <set name="atlSielhauts" table="atl_sielhaut" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="objektid" not-null="true" />
            </key>
            <one-to-many class="de.bielefeld.umweltamt.aui.mappings.atl.AtlSielhaut" />
        </set>
        <one-to-one name="anh49Fachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh49Fachdaten"></one-to-one>
        <one-to-one name="anh50Fachdaten" class="de.bielefeld.umweltamt.aui.mappings.indeinl.Anh50Fachdaten"></one-to-one>
    </class>
</hibernate-mapping>
