<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.basis" schema="auik">

    <class name="BasisSachbearbeiter" table="basis_sachbearbeiter" lazy="false" mutable="false">
        <id name="kennummer" column="kennummer" type="java.lang.String">
            <generator class="assigned"/>
        </id>

        <property name="name" column="name" type="java.lang.String" />
        <property name="telefon" column="telefon" type="java.lang.String" />
        <property name="gehoertzuarbeitsgr" column="gehoertzuarbeitsgr" type="java.lang.String" />
        <property name="enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="deleted" column="_deleted" type="java.lang.Boolean" />
    </class>

</hibernate-mapping>