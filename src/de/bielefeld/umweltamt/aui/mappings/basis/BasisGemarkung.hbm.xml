<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.basis" schema="auik">

    <class name="BasisGemarkung" table="basis_gemarkung" lazy="false" mutable="false">
        <id name="gemarkungid" column="id" type="java.lang.Integer">
            <generator class="assigned"/>
        </id>

        <property name="gemarkung" column="gemarkung" type="java.lang.String" />
    </class>

</hibernate-mapping>