<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.bielefeld.umweltamt.aui.mappings.basis" schema="auik">

    <class name="BasisBetreiber" table="basis_betreiber" lazy="false">
        <id name="betreiberid" type="java.lang.Integer">
        	<column name="id" sql-type="serial"/>
            <generator class="native">
                <param name="sequence">basis_betreiber_id_seq</param>
        	</generator>
        </id>

        <property name="betranrede" column="betranrede" type="java.lang.String" />
        <property name="betrvorname" column="betrvorname" type="java.lang.String" />
        <property name="betrname" column="betrname" type="java.lang.String" />
        <property name="betrnamezus" column="betrnamezus" type="java.lang.String" />
        <property name="namebetrbeauf" column="namebetrbeauf" type="java.lang.String" />
        <property name="vornamebetrbeauf" column="vornamebetrbeauf" type="java.lang.String" />
        <property name="strasse" column="strasse" type="java.lang.String" />
        <property name="hausnr" column="hausnr" type="java.lang.Integer" />
        <property name="hausnrzus" column="hausnrzus" type="java.lang.String" />
        <property name="plzzs" column="plzzs" type="java.lang.String" />
        <property name="plz" column="plz" type="java.lang.String" />
        <property name="ort" column="ort" type="java.lang.String" />
        <property name="telefon" column="telefon" type="java.lang.String" />
        <property name="telefax" column="telefax" type="java.lang.String" />
        <property name="email" column="email" type="java.lang.String" />
        <property name="bemerkungen" column="bemerkungen" type="java.lang.String" />
        <property name="revidatum" column="revidatum" type="java.util.Date" />
        <property name="revihandz" column="revihandz" type="java.lang.String" />
        <property name="kassenzeichen" column="kassenzeichen" type="java.lang.String" />
        <property name="_enabled" column="_enabled" type="java.lang.Boolean" />
        <property name="_deleted" column="_deleted" type="java.lang.Boolean" />

        <many-to-one name="vawsWirtschaftszweige" column="wirtschaftszweigid" class="de.bielefeld.umweltamt.aui.mappings.vaws.VawsWirtschaftszweige" />
    </class>

</hibernate-mapping>