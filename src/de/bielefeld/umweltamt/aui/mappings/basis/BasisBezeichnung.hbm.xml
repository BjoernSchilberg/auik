<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="de.bielefeld.umweltamt.aui.mappings.basis.BasisBezeichnung" table="basis_bezeichnung" schema="auik">
        <id name="id" type="int">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="gruppe" type="string">
            <column name="gruppe" not-null="true" />
        </property>
        <property name="bezeichnung" type="string">
            <column name="bezeichnung" />
        </property>
        <property name="enabled" type="boolean">
            <column name="_enabled" not-null="true" />
        </property>
        <property name="deleted" type="boolean">
            <column name="_deleted" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
